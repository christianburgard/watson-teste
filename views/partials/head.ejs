	.topnav {
        position:relative;
        z-index:2;
        font-size:17px;
        background-color:#5f5f5f;
        color:#f1f1f1;
        width:100%;
        padding:0;
        letter-spacing:1px;
        font-family:"Segoe UI",Arial,sans-serif;
    }

    .topnav .w3-bar a:hover{
        background-color:#000000 !important;
        color:#ffffff !important;
    }

    .topnav .w3-bar a.active {
        background-color:#149078;
        color:#ffffff;
    }

    /* cover */
    .cover1 {position:absolute; width:100%; height:100%; background:#efefef; opacity:0.7; cursor:wait; z-index:2000; text-align:center; font-size:16px; box-sizing: border-box;}
    .cover1-inner, .cover1-inner2 {box-sizing:border-box;}
    .cover1-inner {width:2.8em;height:2.8em;display:inline-block;}
    .cover1-inner span {text-align: center;}
    .cover1-inner2 {width:100%;height:100%; border:0; overflow:hidden; margin:0;}
    .cover1-inner2 > span {line-height:122%; margin:0; padding:0; display:inline;font-size:36px;}

    /* btn caret */
    .btn-dropdown-change .btn .caret {margin-left:5px;}
